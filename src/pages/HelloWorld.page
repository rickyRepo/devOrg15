<apex:page controller="BirthdayController" >
	
    <style>
        
        header{
        	padding: 40px;
        	font-size: 40px;
        	background-image: linear-gradient(to bottom, #688926, #5e821f, #587a18, #4f7311, #496c08);	
        	text-align: center;
        	color: #fff;
        }
        
    	.birthday{
        	margin: 0 auto;
        	text-align: center;
        	background-image: linear-gradient(to bottom, #658926, #5d821f, #567a18, #4e7311, #476c08);	
        }
        .birthday-avatar{
        	padding-top: 40px;
        }
        .birthday-avatar img{
        	border-radius: 50%;
        	box-shadow: 0 0 10px rgba(0, 0, 0, 0.6); 
        	background: #32bea6;
        }
        .birthday-wish{
        	font-family: "Open Sans", MONOSPACE;
        	font-size: 30px;
        	padding: 10px 0;
        	color: #fff;
        }
        .zen{
        	display: none !important;
        }
    </style>   
    <header>
    	<h1>
            Client and Industries
        </h1>
    </header>
    <div class="birthday">
		
        <div class="birthday-avatar" style="display: {! If(Gender == 'Female', 'block', 'none')}">
            <img src="data:image/png;base64,"
                 height="180" width="180" />
        </div>
        
        <div class="birthday-avatar" style="display: {! If(Gender == 'Male', 'block', 'none')}">
            <img src="data:image/png;base64,"
                 height="180" width="180" />
        </div>
        
        
        
        <div class="birthday-wish" style="display: {! If(Message == 'False', 'block', 'none')}">
           <p>
               Upcoming birthday:  <strong>{!Person}</strong>  on {!BirthDate}   
           </p>  
        </div>
        
        <div class="birthday-wish" style="display: {! If(Message == 'True', 'block', 'none')}">
           <p>
               <strong>Happy Birthday:  {!Person}</strong>.   
           </p>  
        </div>
    </div>
    <author>
    	&copy; SF:RAHUL UI:BHARAT 
    </author>
    	
 
</apex:page>